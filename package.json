{
  "name": "teamspeak.js",
  "version": "1.10.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "docs": "typedoc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamspeakjs/teamspeak.js.git"
  },
  "author": "Lixqa",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/teamspeakjs/teamspeak.js/issues"
  },
  "homepage": "https://teamspeak.js.org",
  "description": "A TypeScript library for interacting with a Teamspeak 3 server using the ServerQuery protocol. It provides a type-safe way for managing server users, channels, and more.",
  "keywords": [
    "teamspeak",
    "teamspeak3",
    "serverquery",
    "typescript",
    "sdk",
    "library",
    "bot",
    "client",
    "ts3"
  ],
  "maintainers": [
    {
      "name": "Lixqa",
      "email": "lixqadev@gmail.com"
    }
  ],
  "dependencies": {
    "@discordjs/collection": "^2.1.1",
    "@vladfrangu/async_event_emitter": "^2.4.6",
    "events": "^3.3.0",
    "ssh2": "^1.17.0",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/node": "^24.3.0",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typedoc": "^0.28.12",
    "typedoc-plugin-coverage": "^4.0.1",
    "vitest": "^3.2.4"
  }
}
